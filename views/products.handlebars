<div class="tableContainer">
  <table class="tProducts">
    <th>ID</th>
    <th>Name</th>
    <th>Description</th>
    <th>Code</th>
    <th>Price</th>
    <th>Stock</th>
    <th>Category</th>
    {{#if isValid}}
    {{#each docs}}
      <tr>
        <td>{{this._id}}</td>
        <td>{{this.title}}</td>
        <td>{{this.description}}</td>
        <td>{{this.code}}</td>
        <td>{{this.price}}</td>
        <td>{{this.stock}}</td>
        <td>{{this.category}}</td>
      </tr>
    {{/each}}
  </table>
  <br/>
<div>
    {{#if hasPrevPage}}
        <a href={{this.prevLink}}>Prev</a>
    {{/if}}
    {{this.page}}
     {{#if hasNextPage}}
        <a href={{this.nextLink}}>Next</a>
    {{/if}} 
</div>
{{else}}
<p>Out of stock</p>
{{/if}}
</div>